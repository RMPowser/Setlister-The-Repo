<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Setlister">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SetlisterTheme">
        <activity
            android:name="com.example.Setlister.Activities.ManagementViewActivity"
            android:label="@string/title_activity_management_view"></activity>
        <activity
            android:name="com.example.Setlister.Activities.MainMenuActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Setlister.Activities.SetlistViewActivity" />
    </application>

</manifest>